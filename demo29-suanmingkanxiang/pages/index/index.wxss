.body {
  /* padding-bottom: 100rpx; */
}

.swiper {
  height: 400rpx;
}

.swiper .slide-image {
  width: 100%;
  height: 400rpx;
}

.swiper .slide-image image {
  width: 100%;
}

.nav {
  flex-wrap: wrap;
  justify-content: center;
  margin-left: -20rpx;
  margin-bottom: 30rpx;
  margin-top: -30rpx;
}

.nav .nav-item {
  flex: 0 0 150rpx;
  background-color: #f55053;
  color: #fff;
  margin: 30rpx 0 0rpx 20rpx;
  padding: 10rpx 20rpx;
  justify-content: center;
  border-radius: 10rpx;
  font-size: 30rpx;
  line-height: 1.5;
}

.nav #share-button {
  margin: 30rpx 0 0rpx 20rpx;
  padding: 10rpx 20rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  line-height: 1.5;
  box-sizing: content-box;
}

.nav #contact-button {
  margin: 30rpx 0 0rpx 20rpx;
  padding: 10rpx 20rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  line-height: 1.5;
  box-sizing: content-box;
}

.deal-number {
  margin: 30rpx 0;
  /* margin-top: -30rpx; */
  margin-bottom: 0;
  border-bottom: 1px solid #f55053;
  border-top: 1px solid #f55053;
  font-size: 30rpx;
  height: 80rpx;
  justify-content: center;
}

.deal-number text {
  color: #f55053;
}

.nav1 {
  flex-wrap: wrap;
  justify-content: center;
  margin-bottom: 30rpx;
  /* margin-top: -30rpx; */
}

.nav1 .nav1-item {
  flex: 0 0 20%;
  font-size: 26rpx;
  height: 180rpx;
  justify-content: center;
  box-sizing: border-box;
}

.nav1-item  .nav1-item-img {
  width: 80rpx;
  height: 80rpx;
  margin-bottom: 20rpx;
}

#scroll-view {
  /*transform: rotate(90deg);*//* background-color: #191939; */
  height: 258px;
  border-radius: 5px;
  color: #999;
  box-sizing: border-box;
  padding: 0 30px;
  font-size: 24rpx;
  overflow: hidden;
  border-top: 1px solid #e1e1e1;
  /* margin-bottom: 60rpx; *//* position: absolute; *//* top: 290rpx; */
}

#scroll-view swiper-item {
  transform: rotate(-90deg);
}

.alone-item {
  height: 50px;
  display: flex;
  align-items: center;
  border-bottom: 1px dotted #e1e1e1;
  box-sizing: border-box;
  z-index: 0;
}

.alone-item-even {
  background-color: #f7f8f9;
}

.alone-number {
  font-size: 10px;
  width: 26px;
  height: 26px;
  border: 1px solid #e1e1e1;
  border-radius: 50%;
  margin-right: 16px;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-basis: 26px;
}

.alone-text {
  overflow: hidden;
  flex: 0 0 60%;
}

.alone-border {
  margin-left: auto;
}

.read-beGet {
  margin-left: 5px;
  flex-basis: 100rpx;
}

#swiper2 {
  height: 100rpx;
  margin-bottom: 30rpx;
  padding: 0 30rpx;
  margin-top: -30rpx;
  font-size: 30rpx;
  color: #333;
  border-bottom: 1px solid #f55053;
}

#swiper2 swiper-item {
  justify-content: space-between;
}

#swiper2 .read-beGet {
  margin-left: 5px;
  flex-basis: 120rpx;
  text-align: right;
}

.red-modal {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  justify-content: center;
}
.red-modal .modal-bg{
  background-color: rgba(0, 0, 0, .4);
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  position: absolute;
}
.red-modal .red-ticket {
  width: 549rpx;
}

.red-modal .red-panel {
  position: relative;
}

.red-modal .get-now {
  position: absolute;
  /* top: 0; */
  right: 0;
  left: 50%;
  margin-left: -136rpx;
  width: 272rpx;
  bottom: 108rpx;
}

.member-list {
  margin-bottom: 30rpx;
}

.member-item {
  margin-bottom: 30rpx;
  padding: 30rpx;
  background-color: #fff;
  padding-bottom: 0;
  box-shadow: 6rpx 8rpx 2rpx #999;
}

.member-top-left {
  margin-right: 30rpx;
  height: 200rpx;
}

.member-top-left .logo {
  width: 120rpx;
  border-radius: 50%;
}

.member-top-left .flex-center {
  font-size: 30rpx;
  height: 80rpx;
}

.member-top-right {
  font-size: 30rpx;
  align-items: flex-start;
  /* height: 100%; */
  height: 200rpx;
  color: #999;
}

.member-top-right view {
  font-weight: bold;
  color: #333;
  line-height: 60rpx;
}

.member-top-right text {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}

.member-top-left .flex-center image {
  width: 40rpx;
  margin-right: 10rpx;
}

.member-top-left .member-level-green {
  color: #9ad34d;
}

.member-top-left .member-level-blue {
  color: #4e99d3;
}

.member-top-left .member-level-purple {
  color: #d14eb0;
}

.member-top-left .member-level-red {
  color: #f55053;
}

.member-item-bottom {
  font-size: 30rpx;
  color: #999;
  border-top: 1px solid #e1e1e1;
}

.member-bottom-status, .member-bottom-order, .member-bottom-question,
.member-bottom-active {
  flex: 1;
  height: 80rpx;
  line-height: 80rpx;
}

.member-bottom-question {
  color: #e7d990;
}

.member-bottom-status image, .member-bottom-order image {
  width: 40rpx;
  margin: 0 8rpx;
}

.show-order{
  position: fixed;
  top: 60rpx;
  left: 30rpx;
  background-color: rgba(0, 0, 0, .6);
  height: 60rpx;
  border-radius: 10rpx;
  font-size: 26rpx;
  color: #fff;
}
.show-order .user-logo{
  width: 50rpx;
  border-radius: 50%;
  margin: 0 10rpx;
}

.show-order .lookDetail{
  width: 40rpx;
  margin: 0 10rpx;
}

.show-order text{
  margin-right: 10rpx;
  font-weight: bold;
}